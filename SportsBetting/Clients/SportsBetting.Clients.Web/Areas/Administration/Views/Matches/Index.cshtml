@using SportsBetting.Clients.Web.Areas.Administration.Models.Matches

@model IEnumerable<MatchViewModel>
@{
    ViewBag.Title = "Matches";
}

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Key</th>
            <th scope="col">Start Time</th>
            <th scope="col">Status</th>
            <th scope="col">Score</th>
            <th scope="col">Stream URL</th>
            <th scope="col">Category Id</th>
            <th scope="col">Tournament Id</th>
            <th scope="col">Home Team Id</th>
            <th scope="col">Away Team Id</th>
            <th scope="col">Created On</th>
            <th scope="col">Deleted On</th>
            <th scope="col">Updated On</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var match in Model)
        {
            <tr>
                <th scope="row">@match.Id</th>
                <td>@match.Key</td>
                <td>@match.StartTime</td>
                <td>@match.Status</td>
                <td>@match.Score</td>
                <td>@match.StreamURL</td>
                <td>@match.CategoryId</td>
                <td>@match.TournamentId</td>
                <td>@match.HomeTeamId</td>
                <td>@match.AwayTeamId</td>
                <td>@match.CreatedOn</td>
                <td>@match.DeletedOn</td>
                <td>@match.ModifiedOn</td>
            </tr>
        }
    </tbody>
</table>